<% const pokedex = require('../../../common/pokedex').getPokedex(pokemon.generation) %>
<img class="icon partner-pokemon" />
<select class="form-control select-partner-pokemon">
    <% for (let i = 0; i < pokedex.length; i++) { %>
        <% let iconSrc=pokemonIcons[pokedex.FileNames[i].replace(' ', '-').toLowerCase()][pokemon.shiny]; %>
        <option value="<%- pokedex[i] %>" data-img="<%- iconSrc %>">
            <%- pokedex[i] %>
        </option>
    <% } %>
</select>